#include<stdio.h>
int main(){
	 
	 int i;

	 for(i=0;i<=6;i++){
	 	 if(i%2==0){
	 	 	printf("\t \n %d",i);
		  }
	}
		 for(i=0;i<=6;i++){
	 	 	printf("\t \n %d",i);
		 
	}
	for(i=0;i<=6;i++){
	 	 	printf("\t \n %d",i);
		 
	}
	
	 for(i=0;i<=6;i++){
	 	 if(i%2==0){
	 	 	printf("\t \n %d",i);
		  }
	
}
	 return 0;
}
