#include <stdio.h>

int main() {
int temp,n,arr[n],i,j;
    
    printf("enter size of an array");
     scanf("%d",&n);
    
    for(i=0;i<n;i++){
        scanf("%d",&arr[i]);

        for(j=0;j<n;j++){
            if(arr[i]>arr[j]){
                
                temp=arr[i];
                 arr[i]=arr[j];
                arr[j]=temp;
            }
        }
    }
    for(i=0;i<n;i++){
          printf ("%d",arr[i]);
    }
  
    return 0;
}
