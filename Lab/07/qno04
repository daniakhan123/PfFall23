//Descriptiom: MAtrix is symmetric or not

#include<stdio.h>
int main(){
	int r,c,i,j,arr[r][c];
	
		printf("\nrows of Matrix :");
		scanf("%d",&r);
		printf("\ncolumns of Matrix :");
		scanf("%d",&c);
		
		
		for(i=0;i<r;i++){
			for(j=0;j<c;j++){
			printf("enter number at [%d] [%d] ",i,j);
			scanf("%d",&arr[i][j]);
		}

	}
		for(i=0;i<r;i++){
			for(j=0;j<c;j++){
				
			printf("%d",arr[i][j]);	
	}	
		printf("\n");	
	}
	
	
		for(i=0;i<c;i++){
			for(j=0;j<r;j++){
				
			printf("%d",arr[j][i]);	
			
	}
		printf("\n");	
	}
	
	if(arr[i][j]==arr[j][i]){
	    printf("matrix is symmetric");
	    
	}
	else {
	    printf("matrix is not symmetric");
	}
	return 0;
}

