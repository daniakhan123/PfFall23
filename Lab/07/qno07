#include<stdio.h>
int main(){
	int arr[2][2];
	int i,j,k,l;
	
	for(i=0;i<2;i++){
		for(j=0;j<2;j++){
			printf("enter elements of %d row and %d column of 1st matrix :",i,j);
			scanf("%d",&arr[i][j]);
		}
	}
	
	
	for(k=0;k<2;k++){
		for(l=0;l<2;l++){
			printf("enter elements of %d row and %d columnof 2nd matrix:",k,l);
			scanf("%d",&arr[k][l]);
		}
	}
	
	
	
	return 0;
}
